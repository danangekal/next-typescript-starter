{
  "name": "next-typescript-starter",
  "version": "0.1.0",
  "description": "Unofficial Next.js + Typescript starter with a latest package",
  "author": "Danang Eko Alfianto <github.com/danangekal>",
  "homepage": "https://github.com/danangekal/next-typescript-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danangekal/next-typescript-starter.git"
  },
  "bugs": {
    "url": "https://github.com/danangekal/next-typescript-starter/issues"
  },
  "license": "MIT",
  "keywords": [
    "nextjs",
    "reactjs",
    "typescript",
    "docker",
    "eslint",
    "prettier",
    "husky",
    "lint-staged"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "telemetry": "next telemetry",
    "lint": "eslint --ignore-path .gitignore \"**/*.+(js|ts|tsx)\" --quiet --fix",
    "format": "prettier --ignore-path .gitignore \"**/*.+(js|ts|tsx|css|scss|json|md|html)\" --write",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "echo \"Write tests!\" && exit 0",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@types/react": "^17.0.27",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "pinst": "^2.1.6",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=12"
  }
}
